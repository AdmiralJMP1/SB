
html
    line-height 1
    font-size 16px


//body
//    font-family 'Open Sans'


h1,h2,h3,h4,h5,h6
    margin 0


ul,li
    margin 0
    padding 0
    list-style-type none

p
    margin 0

a
    text-decoration none
    color black

audio,
canvas,
iframe,
img,
svg,
video
    vertical-align middle


textarea
    resize none


section
    position relative


input,
select,
button
    outline none


*, *:before, *:after
    box-sizing border-box


// 1920 // 1530

.main-wrapper
    max-width 1920px
    width 100%
    margin 0px auto

.wrapper
    max-width 1530px
    width 100%
    margin 0px auto
// overflow hidden
// position relative

.clearfix:before,
.clearfix:after
    content: " ";
    display: table;

.clearfix:after
    clear: both;

.clearfix
    *zoom: 1;
